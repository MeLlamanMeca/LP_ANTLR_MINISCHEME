(define (main)
    ; Consult NOT tests
    (display (not #t)) (newline)
    (display (not #f)) (newline)

    ; Consult NUMBER? tests
    (display (number? 42)) (newline)
    (display (number? "hello")) (newline)

    ; Consult BOOLEAN? tests
    (display (boolean? #t)) (newline)
    (display (boolean? 42)) (newline)

    ; Consult STRING? tests
    (display (string? "hello")) (newline)
    (display (string? 42)) (newline)

    ; Consult LIST? tests
    (display (list? '(1 2 3))) (newline)
    (display (list? 42)) (newline)
)